{
  "name": "agent-infrastructure",
  "version": "1.0.0",
  "description": "Complete infrastructure for building production-grade AI agent systems",
  "private": true,
  "workspaces": [
    "core-agent-frameworks",
    "agent-orchestration",
    "memory-state-management",
    "evaluation-testing",
    "deployment-serving",
    "multi-agent-systems",
    "tool-use-integration",
    "llm-providers",
    "vector-databases",
    "advanced-agent-patterns",
    "advanced-tools",
    "ui-dashboard",
    "cli",
    "agent-templates"
  ],
  "scripts": {
    "test": "npm run test --workspaces --if-present",
    "test:coverage": "npm run test -- --coverage --coverageDirectory=coverage",
    "test:watch": "npm run test -- --watch",
    "build": "npm run build --workspaces --if-present",
    "dev": "npm run dev --workspaces --if-present",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "clean": "rm -rf node_modules && rm -rf */node_modules && rm -rf */dist",
    "version": "npm version patch && git push --follow-tags",
    "publish:all": "npm publish --workspaces --access public"
  },
  "keywords": [
    "agent",
    "llm",
    "ai",
    "langchain",
    "autogen",
    "crewai",
    "multi-agent",
    "orchestration",
    "vector-store",
    "mcp",
    "rag",
    "ai-infrastructure"
  ],
  "author": "Yoshi Kondo <yoshi@musicailab.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yksanjo/agent-infrastructure.git"
  },
  "bugs": {
    "url": "https://github.com/yksanjo/agent-infrastructure/issues"
  },
  "homepage": "https://github.com/yksanjo/agent-infrastructure#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
